<Window x:Class="MusicBucket.UserControls.FrameDisplayDialog"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:myProps="clr-namespace:MusicBucket.Properties"
      xmlns:mb="clr-namespace:MusicBucket.UserControls"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
        Width="400" Height="600"
	Title="{x:Static myProps:Resources.frameListDialogTitle}">
    <Grid>
        <ScrollViewer>
            <mb:FrameListDisplay x:Name="fldisp" Height="Auto" />
        </ScrollViewer>
    </Grid>
</Window>
